{
  "name": "xn-lr-web",
  "version": "1.16.7",
  "author": "shenghui <shenghui@shubidatech.com>",
  "private": true,
  "scripts": {
    "start-h5": "ng serve --host 0.0.0.0 --proxy-config=proxy.conf.json",
    "start": "npm run ng serve -- --project=xn-lr-web --open --proxy-config=proxy.conf.json",
    "rex": "npm run ng serve -- --project=xn-lr-web --open --proxy-config=/proxy/rex.json",
    "rexbuild": "npm run ng build --dev",
    "t5": "npm run ng serve -- --project=xn-lr-web --open  --proxy-config=proxy.conf-t5.json",
    "dev": "npm run ng serve -- --project=xn-lr-web --open  --proxy-config=proxy.conf-dev.json",
    "test": "npm run ng serve -- --project=xn-lr-web --open  --proxy-config=proxy.conf-test.json",
    "exp": "npm run ng serve -- --project=xn-lr-web --open  --proxy-config=proxy.conf-exp.json",
    "build": "npm run ng build -- --project=xn-lr-web --prod",
    "start:hz": "npm run ng serve -- --project=hz-web --open --proxy-config=proxy.conf.json",
    "build:hz": "npm run ng build -- --project=hz-web --prod",
    "build:blh": "npm run ng build -- --project=xn-blh-web --prod",
    "start:blh": "npm run ng serve -- --project=xn-blh-web --open --proxy-config=proxy.conf.json",
    "lint": "npm run ng lint",
    "new": "npm run ng generate library --entry-file=index --",
    "mock": "npm run ng serve -- --project=mock --open --proxy-config=proxy.conf.json",
    "ng": "node --max_old_space_size=16384 node_modules/@angular/cli/bin/ng"
  },
